POST renovacionestopup/_search
{
  "query": {
    "bool": {
      "must": [
        {
          "bool": {
            "must": [
              {
                "match": {
                  "SAP_estatus_cliente": 37
                }
              }
            ],
            "must_not": [
              {
                "match": {
                  "SAP_subestatus_cliente": {
                    "query": "(40) OR (41)"
                  }
                }
              }
            ]
          }
        },
        {
          "bool": {
            "should": [
              {
                "bool": {
                  "must": [
                    {
                      "bool": {
                        "should": [
                          {
                            "match": {
                              "etapa_cliente": "contratado"
                            }
                          },
                          {
                            "match": {
                              "etapa_cliente": "solicitado"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "match": {
                        "estatus_cliente": "rechazado"
                      }
                    }
                  ]
                }
              },
              {
                "bool": {
                  
                }
              }
            ]
          }
        },
        {
          "bool": {
            "must": [
              {
                "match": {
                  "idCliente": 26
                }
              },
              {
                "match": {
                  "id_campania": "1"
                }
              }
            ]
          }
        }
      ]
    }
  }
